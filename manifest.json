{
  "manifest_version": 1,
  "version": "1.0.0",
  "name": "Chrome Extension ODD",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.[TARGET_HOMEPAGE].com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["storage", "tabs", "scripting", "webNavigation"],
  "host_permissions": ["https://*.[TARGET_HOMEPAGE].com/*"]
}
